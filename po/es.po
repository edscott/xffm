#. 0x01
#: .././primary/primary-options.i:37 .././primary/primary-options.i:234
#: .././primary/primary-options.i:329 .././primary/primary-options.i:371
#: .././primary/primary-options.i:428 .././primary/primary-options.i:477
#: .././modules/settings-module.i:2857
msgid "Ask the user to get additional parameters"
msgstr "Preguntar al usuario para obtener parámetros adicionales"

#. 0x02
#: .././primary/primary-options.i:39
msgid "-A, --almost-all           do not list implied . and .."
msgstr "-A, --almost-all           no muestra las entradas . y .. implícitas"

#. 0x04
#: .././primary/primary-options.i:42
msgid "-C                         list entries by columns"
msgstr "-C                         muestra las entradas por columnas"

#. 0x08
#: .././primary/primary-options.i:44
msgid ""
"-D, --dired                generate output designed for Emacs' dired mode"
msgstr ""
"-D, --dired                genera el resultado para el modo `dired' de Emacs"

#. 0x010
#: .././primary/primary-options.i:46
msgid "-F, --classify             append indicator (one of */=>@|) to entries"
msgstr ""
"-F, --classify             añade un indicador (uno de */=@|) a las entradas"

#. 0x020
#: .././primary/primary-options.i:48
msgid "-G, --no-group             in a long listing, don't print group names"
msgstr ""
"-G, --no-group             en un listado largo, no muestra nombres de grupo"

#. 0x040
#. 0x02
#: .././primary/primary-options.i:50 .././primary/primary-options.i:237
msgid ""
"-H, --dereference-command-line\n"
"follow symbolic links listed on the command line"
msgstr ""
"-H, --dereference-command-line\n"
"sigue los enlaces simbólicos en la línea de\n"
"órdenes"

#. 0x080
#: .././primary/primary-options.i:52
msgid ""
"-I, --ignore=PATTERN       do not list implied entries matching shell PATTERN"
msgstr ""
"-I, --ignore=PATRÓN        no lista las entradas que coincidan (encajen)\n"
"con PATRÓN de shell"

#. 0x0100
#: .././primary/primary-options.i:54
msgid ""
"-L, --dereference          when showing file information for a symbolic\n"
"link, show information for the file the link\n"
"references rather than for the link itself"
msgstr ""
"-L, --dereference          al mostrar la información de un fichero para un\n"
"enlace simbólico, muestra la información del\n"
"fichero al que apunta el enlace en lugar de la\n"
"del propio enlace"

#. 0x0200
#: .././primary/primary-options.i:56
msgid ""
"-N, --literal              print raw entry names (don't treat e.g. control\n"
"characters specially)"
msgstr ""
"-N, --literal              muestra los nombres literalmente (no trata p. "
"ej.\n"
"los caracteres de control de forma especial)"

#. 0x0400
#: .././primary/primary-options.i:58
msgid "-Q, --quote-name           enclose entry names in double quotes"
msgstr ""
"-Q, --quote-name           encierra los nombres de las entradas entre\n"
"comillas"

#. 0x0800
#: .././primary/primary-options.i:60
msgid "-R, --recursive            list subdirectories recursively"
msgstr "-R, --recursive            muestra los subdirectorios recursivamente"

#. 0x01000
#: .././primary/primary-options.i:62
msgid "-S                         sort by file size"
msgstr "-S                         ordena los ficheros por tamaño"

#. 0x02000
#: .././primary/primary-options.i:64
msgid "-T, --tabsize=COLS         assume tab stops at each COLS instead of 8"
msgstr ""
"-T, --tabsize=COLS         establece los topes de tabulación a cada COLS\n"
"en lugar de 8"

#. 0x04000
#: .././primary/primary-options.i:66
msgid "-U                         do not sort; list entries in directory order"
msgstr ""
"-U                         no ordena; muestra las entradas en el orden del\n"
"directorio"

#. 0x08000
#: .././primary/primary-options.i:68
msgid "-X                         sort alphabetically by entry extension"
msgstr ""
"-X                         ordena alfabéticamente por la extensión de la\n"
"entrada"

#. 0x010000
#: .././primary/primary-options.i:70
msgid ""
"-Z, --context              print any SELinux security context of each file"
msgstr ""
"-Z, --context              muestra el contexto de seguridad SELinux de cada\n"
"fichero"

#. 0x020000
#: .././primary/primary-options.i:72
msgid "-a, --all                  do not ignore entries starting with ."
msgstr "-a, --all                  no oculta las entradas que comienzan con ."

#. 0x040000
#: .././primary/primary-options.i:74
msgid "--author               with -l, print the author of each file"
msgstr "--author               con -l, imprime el autor de cada fichero"

#. 0x080000
#: .././primary/primary-options.i:76
msgid ""
"-b, --escape               print C-style escapes for nongraphic characters"
msgstr ""
"-b, --escape               imprime escapes en estilo C para los caracteres "
"no\n"
"gráficos"

#. 0x0100000
#: .././primary/primary-options.i:80
msgid ""
"-c                         with -lt: sort by, and show, ctime (time of last\n"
"modification of file status information)\n"
"with -l: show ctime and sort by name\n"
"otherwise: sort by ctime, newest first"
msgstr ""
"-c                         con -lt: ordena por ctime y muestra ctime (fecha\n"
"de última modificación del fichero)\n"
"con -l: muestra ctime y ordena por nombre\n"
"en cualquier otro caso: ordena por ctime, el\n"
"más reciente primero"

#. 0x0200000
#: .././primary/primary-options.i:82
msgid ""
"--color[=WHEN]         colorize the output.  WHEN defaults to 'always'\n"
"or can be 'never' or 'auto'.  More info below"
msgstr ""
"--color[=CUÁNDO]       colorea la salida. Por omisión CUÁNDO es `always'\n"
"y puede ser también `never' o `auto'.\n"
"Más información abajo."

#. 0x0400000
#: .././primary/primary-options.i:84
msgid ""
"-d, --directory            list directory entries instead of contents,\n"
"and do not dereference symbolic links"
msgstr ""
"-d, --directory            muestra las entradas de los directorios en lugar\n"
"de sus contenidos, y no sigue los enlaces\n"
"simbólicos"

#. 0x0800000
#: .././primary/primary-options.i:86
msgid "-f                         do not sort, enable -aU, disable -ls --color"
msgstr ""
"-f                         no ordena, utiliza -aU, no utiliza -ls --color"

#. 0x01000000
#: .././primary/primary-options.i:88
msgid "--file-type            likewise, except do not append '*'"
msgstr "--file-type            similar, pero no añade `*'"

#. 0x02000000
#: .././primary/primary-options.i:90
msgid ""
"--format=WORD          across -x, commas -m, horizontal -x, long -l,\n"
"single-column -1, verbose -l, vertical -C"
msgstr ""
"--format=PALABRA       across -x, commas -m, horizontal -x, long -l,\n"
"single-column -1, verbose -l, vertical -C"

#. 0x04000000
#: .././primary/primary-options.i:92
msgid "--full-time            like -l --time-style=full-iso"
msgstr "--full-time            como -l --time-style=full-iso"

#. 0x08000000
#: .././primary/primary-options.i:94
msgid "-g     like -l, but do not list owner"
msgstr "  -g     como -l, pero no lista el propietario"

#. 0x010000000
#: .././primary/primary-options.i:96
msgid "group directories before files."
msgstr "agrupa carpetas antes de los archivos."

#. 0x020000000
#: .././primary/primary-options.i:98
msgid ""
"-h, --human-readable       with -l, print sizes in human readable format\n"
"(e.g., 1K 234M 2G)"
msgstr ""
"-h, --human-readable       con -l, imprime los tamaños en formato legible\n"
"(p. ej. 1K 234M 2G)"

#. 0x040000000
#: .././primary/primary-options.i:100
msgid "--si                   likewise, but use powers of 1000 not 1024"
msgstr ""
"-H, --si                   análogo, pero utiliza potencias de 1000 y no de "
"1024"

#. 0x080000000
#: .././primary/primary-options.i:102
msgid ""
"--dereference-command-line-symlink-to-dir\n"
"follow each command line symbolic link\n"
"that points to a directory"
msgstr ""
"--dereference-command-line-symlink-to-dir\n"
"sigue cada enlace simbólico en la línea de\n"
"órdenes que apunte a un directorio"

#. 0x0100000000
#: .././primary/primary-options.i:104
msgid ""
"--hide=PATTERN         do not list implied entries matching shell PATTERN\n"
"(overridden by -a or -A)"
msgstr ""
"--hide=PATRÓN          no lista las entradas implícitas que coinciden\n"
"con el patrón de shell PATRÓN\n"
"(las opciones -a o -A tienen prioridad)"

#. 0x0200000000
#: .././primary/primary-options.i:106
msgid ""
"--indicator-style=WORD  append indicator with style WORD to entry names:\n"
"none (default), slash (-p),\n"
"file-type (--file-type), classify (-F)"
msgstr ""
"--indicator-style=PALABRA  añade un indicador con estilo PALABRA a los\n"
"nombres de las entradas: none (predeterminado),\n"
"slash (-p), file-type (--file-type), classify (-F)"

#. 0x0400000000
#: .././primary/primary-options.i:108
